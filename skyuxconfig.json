{
  "$schema": "node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-progress-indicator",
  "mode": "easy",
  "compileMode": "aot",
  "app": {
    "styles": [
      "@skyux/theme/css/sky.css",
      "@skyux/docs-tools/css/docs-tools.css"
    ]
  },
  "skyuxModules": [
    "SkyErrorModule"
  ],
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  }
}
